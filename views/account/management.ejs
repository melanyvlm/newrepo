<h1>
    <%= title %>
</h1>


<% errors=(typeof errors !=='undefined' && errors !==null) ? errors : [] %>
    <% if (errors.length> 0) { %>
        <ul class="notice">
            <% errors.forEach(error=> { %>
                <li>
                    <%= error.msg %>
                </li>
                <% }) %>
        </ul>
        <% } %>
        <h2>you are logged in</h2>